#include <stdio.h>

int main() {
    int n, k, i, j;
    int arr[200];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter k: ");
    scanf("%d", &k);

    if(k > n || k <= 0) {
        printf("-1\n");
        return 0;
    }

    for(i = 0; i <= n - k; i++) {
        int found = 0;

        for(j = i; j < i + k; j++) {
            if(arr[j] < 0) {
                printf("%d ", arr[j]);
                found = 1;
                break;
            }
        }

        if(found == 0) {
            printf("0 ");
        }
    }

    printf("\n");
    return 0;
}
