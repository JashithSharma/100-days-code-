#include <stdio.h>

int main() {
    int r, c, i, j, sum = 0;
    int a[50][50];

    printf("Enter number of rows: ");
    scanf("%d", &r);

    printf("Enter number of columns: ");
    scanf("%d", &c);

    printf("Enter the elements of the matrix:\n");
    for(i = 0; i < r; i++) {
        for(j = 0; j < c; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    for(i = 0; i < r; i++) {
        for(j = 0; j < c; j++) {
            sum = sum + a[i][j];
        }
    }

    printf("Sum of all elements = %d\n", sum);

    return 0;
}
