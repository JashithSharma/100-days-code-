#include <stdio.h>

enum Role {
    ADMIN = 1,
    USER,
    GUEST
};

int main() {
    enum Role r;

    for(r = ADMIN; r <= GUEST; r++) {
        if(r == ADMIN)
            printf("ADMIN = %d\n", r);
        else if(r == USER)
            printf("USER = %d\n", r);
        else if(r == GUEST)
            printf("GUEST = %d\n", r);
    }

    return 0;
}
