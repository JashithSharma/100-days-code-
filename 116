#include <stdio.h>

int main() {
    int n, target, i, j;
    int nums[200];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d positive integers:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }

    printf("Enter target: ");
    scanf("%d", &target);

    for(i = 0; i < n; i++) {
        for(j = i + 1; j < n; j++) {
            if(nums[i] + nums[j] == target) {
                printf("%d %d\n", i, j);
                return 0;
            }
        }
    }

    printf("-1 -1\n");
    return 0;
}
