#include <stdio.h>

int main() {
    int n, i, j;
    int arr[200];
    int prev[200];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < n; i++) {
        prev[i] = -1;
        for(j = i - 1; j >= 0; j--) {
            if(arr[j] > arr[i]) {
                prev[i] = arr[j];
                break;
            }
        }
    }

    printf("Previous greater elements: ");
    for(i = 0; i < n; i++) {
        printf("%d", prev[i]);
        if(i != n - 1)
            printf(",");
    }
    printf("\n");

    return 0;
}
