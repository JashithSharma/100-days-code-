#include <stdio.h>

int main() {
    int n, i, j;
    int a[50][50];

    printf("Enter size of square matrix (n): ");
    scanf("%d", &n);

    printf("Enter elements of the matrix:\n");
    for(i = 0; i < n; i++) {
        for(j = 0; j < n; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    for(i = 0; i < n; i++) {
        for(j = 0; j < n; j++) {
            if(a[i][j] != a[j][i]) {
                printf("The matrix is NOT symmetric\n");
                return 0;
            }
        }
    }

    printf("The matrix IS symmetric\n");

    return 0;
}
