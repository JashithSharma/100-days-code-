#include <stdio.h>

int main() {
    int i, j;

    // Block 1: 1 star
    for(i = 1; i <= 1; i++) {
        printf("*\n");
    }
    printf("\n");

    // Block 2: 3 stars
    for(i = 1; i <= 3; i++) {
        printf("*\n");
    }
    printf("\n");

    // Block 3: 5 stars
    for(i = 1; i <= 5; i++) {
        printf("*\n");
    }
    printf("\n");

    // Block 4: 3 stars
    for(i = 1; i <= 3; i++) {
        printf("*\n");
    }
    printf("\n");

    // Block 5: 1 star
    for(i = 1; i <= 1; i++) {
        printf("*\n");
    }

    return 0;
}
