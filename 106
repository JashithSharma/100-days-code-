#include <stdio.h>

int main() {
    int n, i, j;
    int arr[200];
    int next[200];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < n; i++) {
        next[i] = -1;
        for(j = i + 1; j < n; j++) {
            if(arr[j] > arr[i]) {
                next[i] = arr[j];
                break;
            }
        }
    }

    printf("Next greater elements: ");
    for(i = 0; i < n; i++) {
        printf("%d", next[i]);
        if(i != n - 1)
            printf(",");
    }
    printf("\n");

    return 0;
}
