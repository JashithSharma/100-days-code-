#include <stdio.h>

int main() {
    int n;

    printf("Enter a number: ");
    scanf("%d", &n);

    if(n >= 0) {
        if(n == 0) {
            printf("Number is zero\n");
        } else {
            printf("Positive number\n");
        }
    } else {
        printf("Negative number\n");
    }

    return 0;
}
