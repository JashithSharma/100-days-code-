#include <stdio.h>

int main() {
    int m, n, i = 0, j = 0, k = 0;
    int a[200], b[200], merged[400];

    printf("Enter size of first array: ");
    scanf("%d", &m);

    printf("Enter %d sorted elements:\n", m);
    for(i = 0; i < m; i++) {
        scanf("%d", &a[i]);
    }

    printf("Enter size of second array: ");
    scanf("%d", &n);

    printf("Enter %d sorted elements:\n", n);
    for(j = 0; j < n; j++) {
        scanf("%d", &b[j]);
    }

    i = 0;
    j = 0;

    while(i < m && j < n) {
        if(a[i] <= b[j]) {
            merged[k] = a[i];
            i++;
        } else {
            merged[k] = b[j];
            j++;
        }
        k++;
    }

    while(i < m) {
        merged[k] = a[i];
        i++;
        k++;
    }

    while(j < n) {
        merged[k] = b[j];
        j++;
        k++;
    }

    printf("Merged sorted array:\n");
    for(i = 0; i < m + n; i++) {
        printf("%d ", merged[i]);
    }
    printf("\n");

    return 0;
}
