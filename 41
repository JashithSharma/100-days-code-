#include <stdio.h>

int main() {
    int n, first, last, temp, digits = 0;

    printf("Enter a number: ");
    scanf("%d", &n);

    temp = n;
    last = n % 10;

    while(temp != 0) {
        first = temp % 10;
        temp = temp / 10;
        digits++;
    }

    int power = 1;
    for(int i = 1; i < digits; i++)
        power = power * 10;

    int middle = (n % power) / 10;

    int swapped = last * power + middle * 10 + first;

    printf("Number after swapping first and last digit = %d\n", swapped);

    return 0;
}
