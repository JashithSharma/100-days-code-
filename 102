#include <stdio.h>

int main() {
    int n, x, i;
    int arr[200];
    int ans = -1;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d sorted elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter x: ");
    scanf("%d", &x);

    for(i = 0; i < n; i++) {
        if(arr[i] >= x) {
            ans = i;
            break;
        }
    }

    printf("%d\n", ans);

    return 0;
}
