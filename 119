#include <stdio.h>

int main() {
    int n, i;
    int arr[300];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < n; i++) {
        int index = arr[i] < 0 ? -arr[i] : arr[i];

        if(arr[index] < 0) {
            printf("Repeated element: %d\n", index);
            return 0;
        }

        arr[index] = -arr[index];
    }

    printf("No repetition found\n");
    return 0;
}
