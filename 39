#include <stdio.h>

int main() {
    int n, rem, product = 1, flag = 0;

    printf("Enter a number: ");
    scanf("%d", &n);

    while(n != 0) {
        rem = n % 10;

        if(rem % 2 != 0) {
            product = product * rem;
            flag = 1;
        }

        n = n / 10;
    }

    if(flag == 1)
        printf("Product of odd digits = %d\n", product);
    else
        printf("No odd digits present\n");

    return 0;
}
