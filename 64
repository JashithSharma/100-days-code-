#include <stdio.h>

int main() {
    int n, digit;
    int freq[10] = {0};
    int max = 0, maxDigit = 0;

    printf("Enter a number: ");
    scanf("%d", &n);

    if(n < 0)
        n = -n;

    while(n > 0) {
        digit = n % 10;
        freq[digit]++;
        n = n / 10;
    }

    for(int i = 0; i < 10; i++) {
        if(freq[i] > max) {
            max = freq[i];
            maxDigit = i;
        }
    }

    printf("Digit occurring the most times = %d\n", maxDigit);

    return 0;
}
