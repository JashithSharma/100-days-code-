#include <stdio.h>

int main() {
    int a, b;
    int sum, diff, prod;
    float quot;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    sum = a + b;
    diff = a - b;
    prod = a * b;
    quot = (float)a / b;

    printf("Sum = %d\n", sum);
    printf("Difference = %d\n", diff);
    printf("Product = %d\n", prod);
    printf("Quotient = %.2f\n", quot);

    return 0;
}
